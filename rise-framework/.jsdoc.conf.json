{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "systemName": "rise-framework",
    "dateFormat": "MMM DD, YYYY HH:mm:ss",
    "outputSourceFiles": false,
    "outputSourcePath": false,
